{
    "data": {
      "dataset": "simulated",
      "datatype": "balanced"
    },
    "methods": [
      {
        "model": "GroundTruthClassifier",
        "name": "GT",
        "params": {}
      },
      {
        "model": "PooledLogisticClassifier",
        "name": "Pooled-LC",
        "params": {
          "space": {
            "alpha_l1": [
              1e-6,
              1000
            ]
          },
          "types": {
            "alpha_l1": "log-uniform-continuous"
          }
        }
      },
      {
        "model": "BayesMTLClassifier",
        "name": "BayesMTL",
        "params": {
          "space": {
            "alp0": [
              1,
              10
            ],
            "beta0": [
              1,
              100
            ],
            "v0": [
              1,
              10
            ],
            "V0": [
              "weighted"
            ]
          },
          "types": {
            "alp0": "log-uniform-continuous",
            "beta0": "log-uniform-continuous",
            "v0": "log-uniform-continuous",
            "V0": "categorical"
          }
        }
      },
      {
        "model": "MSSLClassifier",
        "name": "MSSL",
        "params": {
          "space": {
            "lambda_1": [
              0.1,
              1000
            ],
            "lambda_2": [
              1e-5,
              100
            ]
          },
          "types": {
            "lambda_1": "log-uniform-continuous",
            "lambda_2": "log-uniform-continuous"
          }
        }
      },
      {
        "model": "LogisticClassifier",
        "name": "STL-LC",
        "params": {
          "space": {
            "penalty": [
              "l1"
            ],
            "C": [
              0.001,
              1
            ]
          },
          "types": {
            "penalty": "categorical",
            "C": "uniform-continuous"
          }
        }
      },
      {
        "model": "JFLMTLClassifier",
        "name": "JFLMTL",
        "params": {
          "space": {
            "rho_L21": [
              0.1,
              3000
            ],
            "rho_L2": [
              0.1,
              3000
            ]
          },
          "types": {
            "rho_L21": "log-uniform-continuous",
            "rho_L2": "log-uniform-continuous"
          }
        }
      }
    ],
    "training": {
      "hyperparameter_selection": {
        "on": true,
        "num_trials": 200,
        "k": 20,
        "metric": "balanced_accuracy",
        "mode": "max",
        "num_cpu": 40
      },
      "metrics": [
        "mcc",
        "balanced_accuracy",
        "accuracy",
        "avg_precision",
        "area_under_curve",
        "f1score",
        "f2score"
      ],
      "resume_training": true,
      "number_runs": 10,
      "exp_name": "MTL-ba-wt_replacement-10repeats"
    }
  }
  